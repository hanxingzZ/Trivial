package testHashMap;

import java.util.Collection;
import java.util.Date;
import java.util.HashMap;
import java.util.Map;

public class CountChar {

	
	public static void reverse(int begin,int end)  {

	   int[] arr = {1,2,3,4,5,6}; 
	    for (; begin < end-1; begin++, end--) {
	    	int temp = arr[end];
	        arr[end] = arr[begin];
	        arr[begin] = temp;
	    }
	    for(int i:arr)
	   System.out.println(i);
	}
	
	public static void main(String[] args) {
		Integer[] q3arr = new Integer[10];
		for (int i = 0; i < q3arr.length; i++) {
			q3arr[i] = i;
		}
		List<Integer> q3list1 = Arrays.asList(q3arr);
		System.out.println("qqqq~" + q3list1.get(9));
		q3arr[9] = 100;
		System.out.println("qqqq~~" + q3list1.get(9));// 此处变为100
	}

}
