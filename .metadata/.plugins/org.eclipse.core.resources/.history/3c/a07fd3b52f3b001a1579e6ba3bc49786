package util;

/**
 * 链表
 *
 */
public class ListNode<T> {

	/**
	 * 数据域
	 */
	private T val;

	/**
	 * 指针域
	 */
	private ListNode<T> next;

	public ListNode() {

	}

	public ListNode(T a) {
		this.val = a;
	}

	public T getVal() {
		return val;
	}

	public void setVal(T val) {
		this.val = val;
	}

	public ListNode<T> getHead() {
		return head;
	}

	public void setHead(ListNode<T> head) {
		this.head = head;
	}

	public ListNode<T> getNext() {
		return next;
	}

	public void setNext(ListNode<T> next) {
		this.next = next;
	}
}
