package leetCode;

public class Ugly {
	public boolean isUgly(int num) {
		if (num < 0) {
			num = ~num + 1;
		}
		if (num == 0 || num == 1) {
			return num == 0 ? false : true;
		}
		while (num != 1) {

		}
		return false;
	}

