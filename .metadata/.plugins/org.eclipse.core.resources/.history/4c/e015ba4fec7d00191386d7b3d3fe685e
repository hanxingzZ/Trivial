package testHashMap;

public class Test2 {

	public static void main(String[] args) {
		int i = 19;
		byte a = (byte) i;
		int a1 = ((int) a) & 0xff;
		int a2 = (int) a;
		String strHex = Integer.toHexString(15);
		String strHex2 = String.format("%0fx", 15);
		System.out.println(strHex);
		System.out.println(strHex2);

	}
}
