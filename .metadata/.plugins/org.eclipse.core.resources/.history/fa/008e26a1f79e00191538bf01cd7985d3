public class CountChar {

	
	public static void reverse(int begin,int end)  {

	   int[] arr = {1,2,3,4,5,6}; 
	    for (; begin < end-1; begin++, end--) {
	    	int temp = arr[end];
	        arr[end] = arr[begin];
	        arr[begin] = temp;
	    }
	    for(int i:arr)
	   System.out.println(i);
	}
	
