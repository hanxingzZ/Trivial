 * @version 1.0
 */
public class LengthOfLIS {
	public int lengthOfLIS(int[] nums) {
		int l = nums.length;
		if (l == 0) {
			return 0;
		}
		int dp[] = new int[l + 1];
		int res = 1;

		for (int i = 1; i < l; i++) {
			for (int j = i - 1; j >= 0; j++) {
				if (nums[j] > nums[i]) {
					dp[i] = Math.max(dp[i], dp[j] + 1);
				}
			}
			res = Math.max(dp[i], res);
		}

		return res;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
