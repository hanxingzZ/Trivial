class Cup {
	Cup(int marker) {
		print("Cup" + marker + ")");
	}

	void f(int marker) {
		print("f" + marker + ")");
	}
}

class Cups {
	static Cup cup1;
	static Cup cup2;
	static {
		cup1 = new Cup(1);
		cup2 = new Cup(2);
	}

	Cups() {
		print("Cups()");
	}
}