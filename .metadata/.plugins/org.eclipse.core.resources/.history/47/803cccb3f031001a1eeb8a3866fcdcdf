public class MajorityElement {
	public static int majorityElement(int[] nums) {
		Map<Integer, Integer> map = new HashMap<>(nums.length / 2);
		for (int a : nums) {
			map.put(a, map.getOrDefault(a, 0) + 1);
		}
		map.forEach(()-{
			
		});
		return 0;
	}

